@using DiffPlex.DiffBuilder.Model
@model SideBySideDiffModel
@{
    ViewBag.Title = "Side By Side Diff";
}

<h2>Side By Side Diff</h2>

<div id="diffBox">
    <div id="leftPane">
        <div class="diffHeader">Old Text</div>        
        @{ Html.RenderPartial("DiffPane", Model.OldText); }
    </div>
    <div id="rightPane">
        <div class="diffHeader">New Text</div>
        @{ Html.RenderPartial("DiffPane", Model.NewText); }
    </div>
    <div class="clear">
    </div>
</div>

<script type="text/javascript">

        $(function() {
            InitializeDiffPanes();
        });
</script>

